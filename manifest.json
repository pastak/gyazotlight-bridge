{
  "name": "Gyazotlight-Bridge",
  "description": "",
  "version": "0.0.3",
  "permissions": [
    "<all_urls>",
    "activeTab",
    "tabs"
  ],
  "background": {
    "scripts": ["main.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["content.js"]
    }
  ],
  "manifest_version": 2
}
